                <?php } ?>
              </select>
            </div>
          </div> #}



          <div class="form-group">
            <label class="col-sm-2 control-label" for="input-status">{{ entry_status }}</label>
            <div class="col-sm-10">
              <select name="payment_paynetz_status" id="input-status" class="form-control">
                {% if payment_paynetz_status %}
                <option value="1" selected="selected">{{ text_enabled }}</option>
                <option value="0">{{ text_disabled }}</option>
                {% else %}
                <option value="1">{{ text_enabled }}</option>
                <option value="0" selected="selected">{{ text_disabled }}</option>
                {% endif %}
              </select>
            </div>
          </div>

        </form>
      </div>
    </div>
  </div>
</div>
{{ footer }}
<script>
function showProxy(){
    var value = $('#isproxy-select option:selected').val();
    if(value == '1'){
        $('#proxy-data').slideDown(500);   
    }else{
        $('#proxy-data').slideUp(500);  
    }
}
</script>