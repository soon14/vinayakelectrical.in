              <input type="text" name="payment_paynetz_productid" value="{{ payment_paynetz_productid }}" id="input-sort-order" class="form-control" />
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label" for="input-status">{{ paynetz_isproxy }}</label>
            <div class="col-sm-10">
              <select onchange="showProxy()" name="payment_paynetz_isproxy" id="isproxy-select" class="form-control">
                {% if paynetz_isproxy == 1 %}
                <option value="1" selected="selected">{{ paynetz_isproxy_yes }}</option>
                <option value="0">{{ paynetz_isproxy_no }}</option>
                {% else %}
                <option value="1">{{ paynetz_isproxy_yes }}</option>
                <option value="0" selected="selected">{{ paynetz_isproxy_no }}</option>
                {% endif %}
              </select>
            </div>
          </div>
            
           <div id="proxy-data" style="display:none">
              <div class="form-group">
                <label class="col-sm-2 control-label" for="input-sort-order">{{ paynetz_proxy_unamepass }}</label>
                <div class="col-sm-10">
                  <input type="text" name="payment_paynetz_proxy_unamepass" value="{{ payment_paynetz_proxy_unamepass }}" id="input-sort-order"
                    class="form-control" />
