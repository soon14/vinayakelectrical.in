            <div class="col-sm-10">
              <select name="payment_paynetz_testmode" id="input-status" class="form-control">
                {% if payment_paynetz_testmode %}
                <option value="1" selected="selected">{{ entry_test_mode_on }}</option>
                <option value="0">{{ entry_test_mode_off }}</option>
                {% else %}
                <option value="1">{{ entry_test_mode_on }}</option>
                <option value="0" selected="selected">{{ entry_test_mode_off }}</option>
                {% endif %}
              </select>
            </div>
          </div>

